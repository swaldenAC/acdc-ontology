<?xml version='1.0' encoding='utf-8' ?>
<rdf:RDF
  xmlns:dcterms="http://purl.org/dc/terms/"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

  <rdf:Description rdf:about='http://acdc.amherst.edu/relationships#'>
    <rdfs:comment>Amherst College Digital Collections - Object Relationship Vocabulary, Version 1.0</rdfs:comment>
    <owl:versionInfo>2015/03/10</owl:versionInfo>
    <rdf:dataType>2015/03/10</rdf:dataType>
  </rdf:Description>

  <rdf:Property rdf:about='http://acdc.amherst.edu/relationships#hasPageNumber'>
    <rdfs:label>Has Page Number</rdfs:label>
    <rdfs:comment>Indicates the literal page number of an object</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about='http://acdc.amherst.edu/relationships#isFollowingPageOf'>
    <rdfs:label>Is Following Page Of</rdfs:label>
    <rdfs:comment>Indicates that the specified subject follows the specified page object</rdfs:comment>
  </rdf:Property>
  
  <rdf:Property rdf:about='http://acdc.amherst.edu/relationships#isPrecedingPageOf'>
    <rdfs:label>Is Preceding Page Of</rdfs:label>
    <rdfs:comment>Indicates that the specified subject is the preceding page of the identified object</rdfs:comment>
  </rdf:Property>

</rdf:RDF>
